model Category {
  id      String    @id @default(cuid())
  name    String    @unique
  Product Product[]
}

model Product {
  id          String   @id
  name        String   @unique
  image       Json
  description String
  price       Int?
  categoryId  String
  Category    Category @relation(fields: [categoryId], references: [id])
}
